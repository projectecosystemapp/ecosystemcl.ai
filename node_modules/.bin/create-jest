#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryleebenson/Desktop/forge-standalone/node_modules/create-jest/bin/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules/create-jest/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules:/Users/ryleebenson/Desktop/node_modules:/Users/ryleebenson/node_modules:/Users/node_modules:/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryleebenson/Desktop/forge-standalone/node_modules/create-jest/bin/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules/create-jest/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules:/Users/ryleebenson/Desktop/node_modules:/Users/ryleebenson/node_modules:/Users/node_modules:/node_modules:/Users/ryleebenson/Desktop/forge-standalone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../create-jest/bin/create-jest.js" "$@"
fi
