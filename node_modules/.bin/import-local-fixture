#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/import-local/fixtures/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/import-local/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules:/home/runner/work/ecosystemcl.ai/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/import-local/fixtures/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/import-local/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules:/home/runner/work/ecosystemcl.ai/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/ecosystemcl.ai/ecosystemcl.ai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
